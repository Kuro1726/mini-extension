{
  "manifest_version": 3,
  "name": "Course extension",
  "version": "1.0",
  "description": "A basic Chrome extension.",
  "action": {
    "default_popup": "./popup/popup.html"
  },
  "permissions": ["storage"],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
